{
  "name": "hdfc-mf-web",
  "version": "1.0.0",
  "description": "HDFC MF WEB",
  "main": "index.js",
  "scripts": {
    "ssr": "NODE_ENV=development babel-node server/app",
    "start": "npm-run-all --parallel start:dev",
    "start:dev": "babel-node tools/run start",
    "open:src": "babel-node tools/srcServer.js",
    "test:local": "mocha --opts mocha.opts",
    "lint": "eslint src",
    "lint:changed": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "lint:changed:fix": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST --fix; fi",
    "lint:watch": "esw src -w",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build:qa": "cross-env NODE_ENV=qa npm run build -- --release",
    "build:uat": "cross-env NODE_ENV=uat npm run build -- --release",
    "build:qa2": "cross-env NODE_ENV=qa2 npm run build -- --release",
    "build:production": "cross-env NODE_ENV=production npm run build -- --release"
  },
  "author": "HDFC-MF-WEB",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bourbon": "4.3.4",
    "browser-sync": "2.18.12",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.24.1",
    "node-bourbon": "4.2.8",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.5",
    "redux-thunk": "2.2.0",
    "resolve-url-loader": "2.0.2",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "webpack-hot-middleware": "2.18.0",
    "webpack-middleware": "1.5.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "1.4.3",
    "del": "^3.0.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "http-proxy": "^1.16.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ncp": "^2.0.0",
    "node-noop": "^1.0.0",
    "npm-run-all": "^1.8.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-infinite-scroll-component": "^2.3.0",
    "react-modal": "^1.9.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "replace": "^0.3.0",
    "react-ga": "^2.2.0"
  }
}
